            "name": "Commit-0",
            "content": "{% extends 'plantilla.html' %}\r\n{% block titulo %} Agregar Boleto {% endblock %}\r\n{% block content %}\r\n    <div class=\"row aling-items-center justify-content-center \">\r\n        <div class=\"col-9 col-md-7 py-5 px-5 border rounded m-4 shadow\">\r\n            <h4 class=\"alert-heading mb-2\">FORMULARIO AGREGAR BOLETO</h4>\r\n            <form action=\"frmAgregarBoleto\" method=\"POST\" class=\"mt-2\">\r\n                <div class=\"form-group mb-2\">\r\n                    <label id=\"id\">Id Boleto: </label>\r\n                    <input type=\"number\" id=\"id\" name=\"id\" class=\"form-control\" required/>\r\n                    <errors path=\"id\" elemnt=\"div\" class=\"text-danger\" />\r\n                </div>\r\n                <div class=\"form-group mb-3\">\r\n                    <label id=\"select_pasajero\">Pasajero: </label>\r\n                    <select class=\"form-select\"  name=\"select_pasajero\" aria-label=\"Default select example\">\r\n                        {% for tipo_persona in tipo_persona %}\r\n                        <option value=\"{{tipo_persona[0]}}\">{{tipo_persona[1]}}</option>\r\n                        {% endfor %}\r\n                    </select>\r\n                </div>\r\n                <div class=\"form-group mb-3\">\r\n                    <label id=\"select_conductor\">Conductor: </label>\r\n                    <select class=\"form-select\" name=\"select_conductor\" aria-label=\"Default select example\">\r\n                        <option selected>Selecciona la ciudad</option>\r\n                        <option value=\"1\">Bogota</option>\r\n                        <option value=\"4\">Medellin</option>\r\n                    </select>\r\n                </div>\r\n                <div class=\"form-group mb-3\">\r\n                    <label id=\"select_viaje\">Viaje: </label>\r\n                    <select class=\"form-select\" name=\"select_viaje\" aria-label=\"Default select example\">\r\n                        <option selected>Selecciona la ciudad</option>\r\n                        <option value=\"1\">Bogota</option>\r\n                        <option value=\"4\">Medellin</option>\r\n                    </select>\r\n                </div>\r\n                <a class=\"btn btn-secondary\" href=\"/listaBoleto\" role=\"button\">Regresar</a>\r\n                <button type=\"submit\" name=\"Enviar\" class=\"btn btn-success\">Agregar</button>\r\n            </form>\r\n        </div>\r\n    </div>\r\n{% endblock %}"
